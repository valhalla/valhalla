{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "dependencies": [
    "boost-headers",
    "boost-property-tree",
    "curl",
    "cxxopts",
    "date",
    {
      "name": "dirent",
      "platform": "windows"
    },
    {
      "name": "gdal",
      "default-features": false
    },
    "geos",
    {
      "name": "libspatialite",
      "default-features": true,
      "features": ["rttopo"]
    },
    "pkgconf",
    "pybind11",
    "rapidjson",
    "spatialite-tools",
    "luajit",
    "lz4",
    "protobuf",
    "sqlite3",
    "zlib",
    "unordered-dense",
    "libosmium",
    "openssl"
  ]
}
